{
  "name": "typescript-starter",
  "version": "1.0.0",
  "description": "Starter code in TypeScript for A0 and beyond",
  "author": "Divya Raj",
  "license": "MIT",
  "main": "out/index.js",
  "type": "module",
  "scripts": {
    "build": "npx tsc",
    "test": "jest --coverage",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint",
    "start": "npm run compile && node ./out/server.js"
  },
  "devDependencies": {
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.2",
    "gts": "^5.2.0",
    "ts-jest": "^29.1.1",
    "typescript": "~5.2.0"
  },
  "dependencies": {
    "@apollo/server": "^4.9.5",
    "apollo-server": "^3.13.0",
    "graphql": "^16.8.1"
  }
}
